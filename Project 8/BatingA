#include <iostream>

using namespace std;

int main(){
    return 0 ;
    int bat[8] ;
    int max,min,av;
    for(int i = 0 ; i<8;i++){
        cout << "Enter a batting average: ";
        cin >> bat[i];
        if(min>bat[i]){
            min = bat[i];
        }
         if(max<bat[i]){
            max = bat[i];
        }
        av=(bat[i]+av);
    }
}